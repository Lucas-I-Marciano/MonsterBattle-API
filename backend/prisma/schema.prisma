generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./monster.db"
}

model Player {
    id    Int     @id @default(autoincrement())
    name String  @unique
}

model Monster {
  id Int @id @default(autoincrement())
  name String @unique
  hp Int
  attack Int
  defense Int
  speed Int
  special String
}

model Arena {
  id Int @id @default(autoincrement())
  name String @unique
  max_players Int
}